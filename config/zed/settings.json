{
	"language_models": {
		"ollama": {}
	},
	"base_keymap": "VSCode",
	"telemetry": {
		"diagnostics": true,
		"metrics": true
	},
	"relative_line_numbers": true,
	"theme": "Catppuccin Frapp√©",
	"buffer_font_family": "FiraCode Nerd Font",
	"tab_size": 2,
	"tabs": {
		"file_icons": true
	},
	"ui_font_size": 15,
	"buffer_font_size": 14,
	"vim_mode": true,
	"terminal": {
		"dock": "right",
		"alternate_scroll": "off",
		"blinking": "terminal_controlled",
		"copy_on_select": false,
		"env": {
			"EDITOR": "zed --wait"
		},
		"font_family": "Liga SFMono Nerd Font",
		"font_size": 13,
		"option_as_meta": false,
		"working_directory": "current_project_directory",
		"line_height": "standard"
	},
	"features": {
		"inline_completion_provider": "supermaven"
	},
	"languages": {
		"TypeScript": {
			"language_servers": ["vtsls", "biome"]
		},
		"TSX": {
			"language_servers": ["vtsls", "biome"]
		},
		"Astro": {
			"language_servers": ["astro-language-server", "vtsls", "biome"]
		},
		"Markdown": {
			"soft_wrap": "editor_width"
		},
		"Prisma": {
			"language_servers": ["prisma-language-server", "biome"]
		}
	},
	"language_servers": ["biome", "!eslint", "!prettier"],
	"formatter": {
		"language_server": {
			"name": "biome"
		}
	},
	"code_actions_on_format": {
		"source.fixAll.biome": true,
		"source.organizeImports.biome": true
	},
	"assistant": {
		"default_model": {
			"provider": "zed.dev",
			"model": "claude-3-5-sonnet-20240620"
		},
		"version": "2",
		"provider": null
	}
}
