{
	"base_keymap": "VSCode",
	"telemetry": {
		"diagnostics": true,
		"metrics": true
	},
	"theme": {
		"mode": "dark",
		"light": "Catppuccin Latte",
		"dark": "Catppuccin Mocha"
	},
	"relative_line_numbers": true,
	"buffer_font_family": "FiraCode Nerd Font",
	"tab_size": 2,
	"tabs": {
		"file_icons": true
	},
	"ui_font_size": 15,
	"buffer_font_size": 14,
	"vim_mode": true,
	"terminal": {
		"dock": "right",
		"alternate_scroll": "off",
		"blinking": "terminal_controlled",
		"copy_on_select": false,
		"env": {
			"EDITOR": "zed --wait",
			"VISUAL": "zed --wait",
			"MANPAGER": "zed --wait +Man!"
		},
		"font_family": "Liga SFMono Nerd Font",
		"font_size": 13,
		"option_as_meta": false,
		"working_directory": "current_project_directory",
		"line_height": "standard"
	},
	"languages": {
		"Astro": {
			"language_servers": [
				"astro-language-server",
				"biome",
				"tailwindcss-language-server",
				"..."
			],
			"prettier": {
				"allowed": false
			}
		},
		"JavaScript": {
			"language_servers": [
				"!typescript-language-server",
				"vtsls",
				"biome",
				"..."
			],
			"prettier": {
				"allowed": false
			}
		},
		"JSON": {
			"language_servers": ["biome", "..."],
			"prettier": {
				"allowed": false
			}
		},
		"JSONC": {
			"language_servers": ["biome", "..."],
			"prettier": {
				"allowed": false
			}
		},
		"Prisma": {
			"formatter": {
				"language_server": {
					"name": "prisma-language-server"
				}
			},
			"language_servers": ["prisma-language-server", "!biome"]
		},
		"TSX": {
			"language_servers": [
				"!typescript-language-server",
				"vtsls",
				"biome",
				"tailwindcss-language-server",
				"..."
			],
			"prettier": {
				"allowed": false
			}
		},
		"TypeScript": {
			"language_servers": [
				"!typescript-language-server",
				"vtsls",
				"biome",
				"tailwindcss-language-server",
				"..."
			],
			"prettier": {
				"allowed": false
			}
		}
	},
	"formatter": {
		"language_server": {
			"name": "biome"
		}
	},
	"language_servers": ["!typescript-language-server", "!eslint", "!prettier"],
	"code_actions_on_format": {
		"source.fixAll.biome": true,
		"source.organizeImports.biome": true
	},
	"features": {
		"inline_completion_provider": "supermaven"
	},
	"assistant": {
		"default_model": {
			"provider": "zed.dev",
			"model": "claude-3-5-sonnet-latest"
		},
		"version": "2",
		"provider": null
	}
}
